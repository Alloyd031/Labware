                                        <SolidColorBrush x:Key="ToggleButtonBackgroundPressed" Color="#EBECED"/>
                                        <SolidColorBrush x:Key="ToggleButtonForegroundPressed" Color="Black"/>
                                        <SolidColorBrush x:Key="ToggleButtonBorderBrushPointerOver" Color="#B3B3B3"/>
                                        <SolidColorBrush x:Key="ToggleButtonBorderBrushPressed" Color="#B3B3B3"/>

                                        <SolidColorBrush x:Key="ToggleButtonBorderBrushChecked" Color="White"/>
                                        <SolidColorBrush x:Key="ToggleButtonBackgroundChecked" Color="White"/>
                                        <SolidColorBrush x:Key="ToggleButtonForegroundChecked" Color="Black"/>
                                        <SolidColorBrush x:Key="ToggleButtonBackgroundCheckedPointerOver" Color="White"/>
                                        <SolidColorBrush x:Key="ToggleButtonForegroundCheckedPointerOver" Color="Black"/>
                                        <SolidColorBrush x:Key="ToggleButtonBackgroundCheckedPressed" Color="White"/>
                                        <SolidColorBrush x:Key="ToggleButtonForegroundCheckedPressed" Color="Black"/>
                                        <SolidColorBrush x:Key="ToggleButtonBorderBrushCheckedPointerOver" Color="White"/>
                                        <SolidColorBrush x:Key="ToggleButtonBorderBrushCheckedPressed" Color="White"/>
                                    </ResourceDictionary>
                                </ResourceDictionary.ThemeDictionaries>
                            </ResourceDictionary>
                        </ToggleButton.Resources>
                        <Grid Margin="-12,-6,-12,-6">
                            <FontIcon Glyph="&#xE18A;" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,0,0,0"/>
                            <TextBlock x:Uid="TestTabString" Text="Test Page" Style="{StaticResource CaptionTextBlockStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="34,0,0,0"/>
                        </Grid>
                    </ToggleButton>
                </StackPanel>
            </Grid>
            <Frame x:Name="TabsFrame" Margin="1,31,2,5" BorderBrush="#69A1BF" BorderThickness="1,1,1,1"/>
        </Grid>
        <Grid x:Name="FolderView" Height="147" BorderBrush="#69A1BF" BorderThickness="1,1,1,1" VerticalAlignment="Bottom" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" Margin="202,0,0,32">
            <Grid.Resources>
                <ResourceDictionary>
                    <ResourceDictionary.ThemeDictionaries>
                        <ResourceDictionary x:Key="Dark">
                            <Color x:Key="SystemAltHighColor">#292929</Color>
                            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="{ThemeResource SystemAltHighColor}"/>
                        </ResourceDictionary>
                        <ResourceDictionary x:Key="Light">
                            <Color x:Key="SystemAltHighColor">#E4E5E7</Color>
                            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="{ThemeResource SystemAltHighColor}"/>
                        </ResourceDictionary>
                    </ResourceDictionary.ThemeDictionaries>
                </ResourceDictionary>
            </Grid.Resources>
            <Grid Margin="1,2,2,2" BorderBrush="#303030" BorderThickness="1,1,1,1" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" Grid.RowSpan="2">
                <Grid.Resources>
                    <ResourceDictionary>
                        <ResourceDictionary.ThemeDictionaries>
                            <ResourceDictionary x:Key="Dark">
                                <Color x:Key="SystemAltHighColor">#1E1E1E</Color>
                                <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="{ThemeResource SystemAltHighColor}"/>
                            </ResourceDictionary>
                            <ResourceDictionary x:Key="Light">
                                <Color x:Key="SystemAltHighColor">White</Color>
                                <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="{ThemeResource SystemAltHighColor}"/>
                            </ResourceDictionary>
                        </ResourceDictionary.ThemeDictionaries>
                    </ResourceDictionary>
                </Grid.Resources>
                <TextBlock Text="No folder view virtual machines." HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1"/>
            </Grid>
            <StackPanel VerticalAlignment="Top" HorizontalAlignment="Right" Padding="0,5,5,0" Margin="0,1,1,0" Grid.RowSpan="2">
                <Button x:Name="HideFolderView" Height="39" Width="39" UseSystemFocusVisuals="False" VerticalContentAlignment="Stretch" Click="HideFolderView_Click" ToolTipService.ToolTip="Hide Folder View" BorderThickness="2,2,2,2" HorizontalContentAlignment="Stretch" IsTabStop="False">
                    <Button.Resources>
                        <ResourceDictionary>
                            <ResourceDictionary.ThemeDictionaries>
                                <ResourceDictionary x:Key="Dark">
                                    <SolidColorBrush x:Key="ButtonBorderBrush" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackground" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="#253F3E"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#215452"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPointerOver" Color="#215452"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPressed" Color="#197F7B"/>
                                </ResourceDictionary>
                                <ResourceDictionary x:Key="Light">
                                    <SolidColorBrush x:Key="ButtonBorderBrush" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackground" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="#CDE8E8"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#B6EAEA"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPointerOver" Color="#89EFED"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPressed" Color="#B6EAEA"/>
                                </ResourceDictionary>
                            </ResourceDictionary.ThemeDictionaries>
                        </ResourceDictionary>
                    </Button.Resources>
                    <FontIcon Glyph="&#xE8BB;" FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center" Height="39" Width="39"/>
                </Button>
                <Button x:Name="FolderViewOptions" Height="39" Width="39" HorizontalContentAlignment="Stretch" IsTabStop="False" ToolTipService.ToolTip="Folder View Options" Margin="0,5,0,0" BorderThickness="2,2,2,2" UseSystemFocusVisuals="False" VerticalContentAlignment="Stretch">
                    <Button.Resources>
                        <ResourceDictionary>
                            <ResourceDictionary.ThemeDictionaries>
                                <ResourceDictionary x:Key="Dark">
                                    <SolidColorBrush x:Key="ButtonBorderBrush" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackground" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="#253F3E"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#215452"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPointerOver" Color="#215452"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPressed" Color="#197F7B"/>
                                </ResourceDictionary>
                                <ResourceDictionary x:Key="Light">
                                    <SolidColorBrush x:Key="ButtonBorderBrush" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackground" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="#CDE8E8"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#B6EAEA"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPointerOver" Color="#89EFED"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPressed" Color="#B6EAEA"/>
                                </ResourceDictionary>
                            </ResourceDictionary.ThemeDictionaries>
                        </ResourceDictionary>
                    </Button.Resources>
                    <FontIcon Glyph="&#xE972;" FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center" Height="39" Width="39"/>
                    <FlyoutBase.AttachedFlyout>
                        <MenuFlyout Placement="BottomEdgeAlignedRight">
                            <RadioMenuFlyoutItem Text="Open Virtual Machines" UseSystemFocusVisuals="False"/>
                            <RadioMenuFlyoutItem Text="Folder View Virtual Machines" IsChecked="True" UseSystemFocusVisuals="False"/>
                        </MenuFlyout>
                    </FlyoutBase.AttachedFlyout>
                </Button>
            </StackPanel>
        </Grid>
        <Border BackgroundSizing="OuterBorderEdge" BorderThickness="0,0,0,0" Height="32" VerticalAlignment="Bottom" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
            <Border.Resources>
                <ResourceDictionary>
                    <ResourceDictionary.ThemeDictionaries>
                        <ResourceDictionary x:Key="Dark">
                            <Color x:Key="SystemAltHighColor">#292929</Color>
                            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="{ThemeResource SystemAltHighColor}"/>
                        </ResourceDictionary>
                        <ResourceDictionary x:Key="Light">
                            <Color x:Key="SystemAltHighColor">#E4E5E7</Color>
                            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="{ThemeResource SystemAltHighColor}"/>
                        </ResourceDictionary>
                    </ResourceDictionary.ThemeDictionaries>
                </ResourceDictionary>
            </Border.Resources>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Height="32">
                <Button UseSystemFocusVisuals="False" CornerRadius="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" ToolTipService.ToolTip="Message log: no messages" Height="32" Width="46" IsTabStop="False">
                    <Button.Resources>
                        <ResourceDictionary>
                            <ResourceDictionary.ThemeDictionaries>
                                <ResourceDictionary x:Key="Dark">
                                    <SolidColorBrush x:Key="ButtonBorderBrush" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackground" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPointerOver" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPressed" Color="Transparent"/>
                                </ResourceDictionary>
                                <ResourceDictionary x:Key="Light">
                                    <SolidColorBrush x:Key="ButtonBorderBrush" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackground" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonForegroundPressed" Color="Black"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPointerOver" Color="Transparent"/>
                                    <SolidColorBrush x:Key="ButtonBorderBrushPressed" Color="Transparent"/>
                                </ResourceDictionary>
                            </ResourceDictionary.ThemeDictionaries>
                        </ResourceDictionary>
                    </Button.Resources>
                    <FontIcon Glyph="&#xE15F;" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Height="32" Width="46"/>
                    <FlyoutBase.AttachedFlyout>
                        <MenuFlyout Placement="TopEdgeAlignedRight">
                            <MenuFlyoutItem Text="Open Message Log" HorizontalAlignment="Stretch" UseSystemFocusVisuals="False" IsTabStop="False"/>
                            <MenuFlyoutItem Text="Clear Messages" HorizontalAlignment="Stretch" UseSystemFocusVisuals="False" IsTabStop="False"/>
                        </MenuFlyout>
                    </FlyoutBase.AttachedFlyout>
                </Button>
            </StackPanel>
        </Border>
    </Grid>
</Window>
